    "js-cookie": "^3.0.5",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-jwt": "^1.2.2",
    "react-router-dom": "^6.26.1",
    "react-toastify": "^10.0.5",
    "swiper": "^11.1.12",
    "yarn": "^1.22.22",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@tailwindcss/typography": "^0.5.15",
